{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": { "~lib/*": ["./src/lib/*"], "~components/*": ["./src/components/*"], "~utils/*": ["./src/utils/*"] }
  },
  "include": ["next-env.d.ts", "./src/**/*.ts", "./src/**/*.tsx", "src/contentful/setup.js"],
  "exclude": ["node_modules"]
}
